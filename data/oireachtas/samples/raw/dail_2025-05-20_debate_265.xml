            <p eId="para_34">
              <person refersTo="#DonnchadhOLaoghaire">Ó Laoghaire, Donnchadh.</person>
            </p>
            <p eId="para_35">
              <person refersTo="#DuncanSmith">Smith, Duncan.</person>
            </p>
            <p eId="para_36">
