        <script>
        // Mailchimp newsletter subscription form
        var f = document.getElementById('newsletter-form'),
            b = document.querySelector('.sd-newsletter__submit'),
            bI = document.querySelector('.sd-newsletter__submit-inner'),
            fdb = document.querySelector('.sd-newsletter__error'),
            email = document.getElementById('email_address'),
            baseUrl = 'https://smartdublin.ie/';

        f.addEventListener('submit', function (e) {
            e.preventDefault();
            console.log(email.value);
            if (email.value.length === 0) {
                fdb.innerHTML = 'Please provide an email address';
                return;
            } else if (!validateEmail(email.value)) {
                fdb.innerHTML = 'Please provide a valid email address';
                return;
            }
            register(email.value);
        });

        function register(data) {
            bI.innerHTML = '<i class="fas fa-spinner-third"></i>';
            bI.classList.add('sd-animation--spinning');
            b.disabled = true;
            var options = {
                method: 'POST',
                headers: new Headers({
                    'Accept': 'application/json',
                    'Content-Type': 'application/json',
                }),
                body: JSON.stringify(data)
            };

            fetch(baseUrl + '/wp-json/gmt-mailchimp/v1/subscribe?email=' + data, options)
                .then(function (response) {
                    return response.json();
                })
                .then(function (data) {
                    console.log('Success:', data);
                    buttonState(data);
                })
                .catch(function (error) {
                    console.error('Error:', error);
                    fdb.innerHTML = 'Unable to subscribe';
                    bI.innerHTML = '<i class="fas fa-times-square"></i>';
                });
        }
        function buttonState(data) {
            bI.classList.remove('sd-animation--spinning');
            switch (data.status) {
                case 'success':
                    fdb.innerHTML = 'Thank you for your subscription';
                    bI.innerHTML = '<i class="fas fa-check-square"></i>';
                    break;
                case 'failed':
                    fdb.innerHTML = 'Unable to subscribe';
                    bI.innerHTML = '<i class="fas fa-times-square"></i>';
                    break;
                default:
                    fdb.innerHTML = '';
                    bI.innerHTML = '<i class="fa fa-arrow-circle-right"></i>';
                    break;
            }

            email.addEventListener('input', function (e) {
                b.disabled = false;
                fdb.innerHTML = '';
                bI.innerHTML = '<i class="fa fa-arrow-circle-right"></i>';
            })
        }
        function validateEmail(email) {
            var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            return re.test(String(email).toLowerCase());
        }
    </script>
    <script>
        // Mobile menu
        var toggle = document.querySelector('.sd-toggle');
        var menu = document.querySelector('.sd-nav-mobile');
        if (toggle !== null || menu !== null) {
            let active = false;

            toggle.addEventListener('click', () => {
                if (active) {
                    open();
                    toggle.classList.remove('sd-active');
                } else {
                    close();
                    toggle.classList.add('sd-active');
                }
            });

            function open() {
                menu.classList.remove('sd-active');
                active = false;
            }

            function close() {
                menu.classList.add('sd-active');
                active = true;
            }
        }
    </script>
