    <script>
        // Handle spacer when header goes position: fixed
        var h = document.querySelector('.sd-header');
        var hS = Array.prototype.slice.call(document.querySelectorAll('.sd-header__spacer'));

        function resizeThrottle(func, delay) {
            var throttled;
            window.addEventListener('resize', function () {
                if (!throttled) {
                    func();
                    throttled = true;
                    setTimeout(function () {
                        throttled = false;
                    }, delay);
                }
            });
        }

        function sizeHeaderSpacer() {
            var vw = Math.max(document.documentElement.clientWidth || 0, window.innerWidth || 0);

            // Only fires when viewport is below 1280px
            if (vw < 1280) {
                var hH = h.offsetHeight;
